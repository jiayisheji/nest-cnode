{
  "name": "nest-cnode",
  "version": "0.2.0",
  "description": "This is using the Nestjs imitation Cnode.",
  "author": {
    "name": "jiayi",
    "email": "690405541@qq.com"
  },
  "keywords": [
    "nest",
    "cnode",
    "node",
    "typescript",
    "express",
    "angular",
    "ejs",
    "jest"
  ],
  "homepage": "https://github.com/jiayisheji/nest-cnode",
  "license": "MIT",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "prestart:prod": "tsc",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "start:hmr": "node dist/server",
    "test": "cross-env NODE_ENV=test jest",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "lint": "tslint \"src/**/*.ts\" --force",
    "assets": "loader /views /",
    "release": "standard-version && git push --follow-tags origin master"
  },
  "dependencies": {
    "@nestjs/common": "^5.3.2",
    "@nestjs/core": "^5.3.3",
    "@nestjs/microservices": "^5.3.2",
    "@nestjs/mongoose": "^5.2.1",
    "@nestjs/passport": "^5.0.1",
    "@nestjs/testing": "^5.3.1",
    "@nestjs/websockets": "^5.3.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.9.1",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dotenv": "^6.0.0",
    "ejs-mate": "^2.3.0",
    "express-session": "^1.15.6",
    "ioredis": "^4.0.0",
    "joi": "^13.6.0",
    "loader": "^2.1.1",
    "loader-builder": "^2.6.1",
    "loader-connect": "^1.0.1",
    "mongoose": "^5.2.9",
    "nodemailer": "^4.6.8",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.2",
    "typescript": "^2.6.2",
    "utility": "^1.14.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/express": "^4.0.39",
    "@types/ioredis": "^3.2.14",
    "@types/jest": "^21.1.8",
    "@types/joi": "^13.4.3",
    "@types/mongoose": "^5.2.6",
    "@types/node": "^9.3.0",
    "@types/nodemailer": "^4.6.2",
    "@types/supertest": "^2.0.4",
    "cross-env": "^5.2.0",
    "husky": "^1.3.1",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "standard-version": "^4.4.0",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  },
  "ci": {
    "version": "8",
    "services": "redis-server, mongodb"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jiayisheji/nest-cnode.git"
  },
  "bugs": {
    "url": "https://github.com/jiayisheji/nest-cnode/issues"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  }
}
