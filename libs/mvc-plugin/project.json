{
  "name": "mvc-plugin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/mvc-plugin/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/mvc-plugin",
        "main": "libs/mvc-plugin/src/index.ts",
        "tsConfig": "libs/mvc-plugin/tsconfig.lib.json",
        "assets": [
          "libs/mvc-plugin/*.md",
          {
            "input": "./libs/mvc-plugin/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/mvc-plugin/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/mvc-plugin",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/mvc-plugin",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/mvc-plugin/**/*.ts",
          "libs/mvc-plugin/generators.json",
          "libs/mvc-plugin/executors.json",
          "libs/mvc-plugin/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/mvc-plugin/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
